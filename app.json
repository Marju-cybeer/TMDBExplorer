{
  "expo": {
    "name": "TMDBExplorer",
    "slug": "TMDBExplorer",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/popcorn.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/popcorn.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/popcorn.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/popcorn.png"
    },
    "plugins": [
      "expo-sqlite",
      "expo-secure-store",
      "expo-font",
      "expo-asset"
    ]
  }
}
